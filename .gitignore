# Replace with a proper .gitignore
cat > .gitignore << 'EOF'
# Secrets and config
config.py
.env
.env.local

# API Credentials
credentials/
*.json
!package.json
!tsconfig.json

# Database (contains personal learned data)
mcp_learning.db
*.db

# Local settings
.claude/

# Python
__pycache__/
*.pyc
*.pyo
*.pyd

# Files folder
files/
files.zip

# Windows
*.lnk
nul

# Google Drive temp files
.tmp.driveupload/

# Mac system files
.DS_Store

# Logs
*.log
EOF